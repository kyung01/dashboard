/* hide input */
input.radio:empty {
	margin-left: -999px;
}

/* style label */
input.radio:empty ~ label {
	position: relative;
	/*float: left;*/
	line-height: 2em;
	text-indent: 35px;
	margin-top: 0;
	cursor: pointer;
	-webkit-user-select: none;
	-moz-user-select: none;
	-ms-user-select: none;
	user-select: none;
}

input.radio:empty ~ label:before {
	position: absolute;
	display: block;
	top: 0;
	bottom: 0;
	left: 0;
	content: '';
	width: 2.5em;
	background: transparent;
	border-radius: 3px 0 0 3px;
	border-right:1px dashed #d1d3d4;
}

/* toggle hover */
input.radio:hover:not(:checked) ~ label:before {
	content:'\2714';
	text-indent: 0;
	color: #fff;
}

input.radio:hover:not(:checked) ~ label {
	color: #fff;
}

/* toggle on */
input.radio:checked ~ label:before {
	content:'\2714';
	text-indent: 0;
	color: #fff;
	box-shadow: 0 0 15px;
}

input.radio:checked ~ label {
	color: #fff;
}

